num1 = 45
num2 = 5
print(f"СЛОЖЕНИЕ: {num1 + num2}")

num1 = 100
num2 = 1
print(f"ВЫЧИТАНИЕ: {num1 - num2}")

num1 = 5
num2 = 4
print(f"УМНОЖЕНИЕ: {num1 * num2}")

num1 = 4
num2 = 3
print(f"ВОЗВЕДЕНИЕ В СТЕПЕНЬ: {num1 ** num2}")

num1 = 69
num2 = 5
print(f"ДЕЛЕНИЕ: {num1 // num2}")

num1 = 30
num2 = 3
print(f"ДЕЛЕНИЕ С ТОЧКОЙ: {num1 / num2}")

num1 = 35
num2 = 9
print(f"ПРОЦЕНТ ОТ ДЕЛЕНИЯ: {num1 % num2}")