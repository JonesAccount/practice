var1, var2 = "A", "B"

print("Первая переменная: ", var1, "\n", "Вторая переменная: ", var2, sep="")

print("\nОбмен значениями...\n")

var1, var2 = var2, var1

print("Первая переменная: ", var1, "\n", "Вторая переменная: ", var2, sep="")

print("\nОбратный процесс...\n")

var1, var2 = var2, var1

print("Первая переменная: ", var1, "\n", "Вторая переменная: ", var2, sep="")